<AnchorPane fx:id="myAnchorPane" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.controller.guicontroller.ControllerGuiNotificationsRestaurantView">
   <children>
      <ImageView fitHeight="700.0" fitWidth="700.0" pickOnBounds="true">
         <image>
            <Image url="@..\restaurant-mural-wallpaper_23-2148707022.jpg" />
         </image>
      </ImageView>
      <Button fx:id="homeButton" layoutX="30.0" layoutY="47.0" mnemonicParsing="false" onAction="#goToHome" prefHeight="31.0" prefWidth="140.0" text="Home" />
      <Button fx:id="chooseRestaurantButton" layoutX="190.0" layoutY="47.0" mnemonicParsing="false" onAction="#chooseRestaurant" prefHeight="31.0" prefWidth="230.0" text="Choose Restaurant" />
      <Button fx:id="scheduleATripButton" layoutX="440.0" layoutY="47.0" mnemonicParsing="false" onAction="#schedule" prefHeight="31.0" prefWidth="230.0" text="Schedule a Trip" />
      <Button fx:id="backButton" layoutX="30.0" layoutY="92.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ImageView fitHeight="51.0" fitWidth="86.0" layoutX="584.0" layoutY="92.0" pickOnBounds="true">
         <image>
            <Image url="@..\utente.jpg" />
         </image>
      </ImageView>
      <Label fx:id="nomeUtenteLabel" layoutX="583.0" layoutY="154.0" text="nomeUtente" textFill="#eeeeee">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TableView fx:id="tabella" editable="true" layoutX="100.0" layoutY="233.0" prefHeight="300.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="colonnaRistorante" minWidth="250.0" prefWidth="75.0" text="Ristorante" />
          <TableColumn fx:id="colonnaNotifica" minWidth="250.0" prefWidth="75.0" text="notifica" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
