            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="writeReviewButton" layoutX="470.0" layoutY="622.0" mnemonicParsing="false" onAction="#goToWriteReviewPage" prefHeight="31.0" prefWidth="200.0" text="Write Review">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <CheckBox layoutX="117.0" layoutY="168.0" mnemonicParsing="false" text="for celiacs" textFill="WHITE" />
      <CheckBox layoutX="277.0" layoutY="168.0" mnemonicParsing="false" text="for vegans" textFill="WHITE" />
